{
  "presets": [["env", { "modules": false }], "react"],
  "env": {
    "test": {
      "presets": [["env"], "react"]
    }
  },
  "plugins": [
    ["transform-class-properties", { "spec": true }],
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "components": "./src/components"
        }
      }
    ]
  ]
}
